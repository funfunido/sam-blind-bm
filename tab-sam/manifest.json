{
    "name": "Tab Sam",
    "description": "When you press TAB, it reads the current element.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "sam.html",
        "default_icon": "favicon.png"
    },
    "commands": {
        "talk": {
            "suggested_key": {
                "default": "Ctrl+Down"
            },
            "description": "When you press TAB, it reads the current element."
        }
    },
    "permissions": ["activeTab"],
    "background": {
        "service_worker": "sam.js"
    }
}
